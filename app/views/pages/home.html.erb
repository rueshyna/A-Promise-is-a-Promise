<% content_for :js_ready do %>
  $("#accordion").accordion({
   active: false,
   collapsible: true
  });
  var goal = $("#goal"),
      howlong = $("#howlong"),
      happen = $("#happen"),
      improvement = $("#improvement"),
      evaluation = $("#evaluation"),
      allFields = $([]).add(goal).add(howlong).add(happen).add(improvement).add(evaluation);
  $('#dialog').dialog({
    height: 570,
    width: 600,
    modal: true,
    autoOpen: false
  });
  $('#pdialog').dialog({
    height: 330,
    width: 450,
    modal: true,
    autoOpen: false
  });
  $('#tips').dialog({autoOpen: false});

  $('#tabs').tabs({
    show: function(event, ui) {
      $('#calendar').fullCalendar('render');
      $('#gcalendar').fullCalendar('render');
    }
  });
  <%= render 'pages/mycalendar' %>
  <%= render 'pages/gcalendar' %>
<% end %>
<%= render 'pages/mydialog' %>
<%= render 'pages/pdialog' %>
<%= render 'pages/tip' %>
<%= render 'pages/tabs' %>

