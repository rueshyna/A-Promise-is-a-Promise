<%= render 'pages/mydialog' %>
<%= render 'pages/tip' %>
<%= render 'pages/tabs' %>

<% content_for :js_ready do %>
  $("#accordion").accordion({
   active: false,
   collapsible: true
  });
  var goal = $("#goal"),
      howlong = $("#howlong"),
      happen = $("#happen")
      improvement = $("#improvement")
      allFields = $([]).add(goal).add(howlong).add(happen).add(improvement);
  $('#dialog').dialog({
    height: 570,
    width: 600,
    modal: true,
    autoOpen: false
  });
  $('#tips').dialog({autoOpen: false});

  $('#tabs').tabs({
    show: function(event, ui) {
      $('#calendar').fullCalendar('render');
      $('#gcalendar').fullCalendar('render');
    }
  });
  $('input[name=user]').get(1).checked = true;
  <%= render 'pages/mycalendar' %>
  <%= render 'pages/gcalendar' %>
<% end %>
